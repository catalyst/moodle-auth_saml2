--TEST--
Twig supports the in operator when using objects
--TEMPLATE--
{% if object in object_list %}
TRUE
{% endif %}
--DATA--
$foo = new Twig\Tests\TwigTestFoo();
$foo1 = new Twig\Tests\TwigTestFoo();

$foo->position = $foo1;
$foo1->position = $foo;

return [
    'object'      => $foo,
    'object_list' => [$foo1, $foo],
]
--EXPECT--
TRUE
